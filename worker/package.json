{
  "name": "@pokedao/worker",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(lint relaxed via root config)\"",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test-integration": "tsx src/test-integration.ts",
    "harvest": "tsx src/smart-collector-crypt-harvester.ts",
    "harvest-recover": "tsx src/smart-collector-crypt-harvester.ts --recover",
    "smart-harvest": "tsx src/resumable-harvester.ts",
    "harvest-reset": "tsx src/resumable-harvester.ts reset"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.11.0",
    "bullmq": "^4.15.4",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "openai": "^5.19.1",
    "pino": "^8.16.2",
    "playwright": "^1.40.0",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^0.34.6"
  }
}
