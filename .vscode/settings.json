{
  "files.associations": {
    "**/*.prisma": "prisma",
    "**/backup/**/*.prisma": "plaintext",
    "**/*.backup": "plaintext",
    "**/*.plan.backup": "plaintext"
  },
  "files.exclude": {
    "**/.pnpm": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/research/**/generated": true,
    "**/research/**/.prisma": true,
    "**/*.backup": true,
    "**/*.plan.backup": true,
    "**/research-backup-*": true
  },
  "search.exclude": {
    "**/.pnpm": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/research/**/generated": true,
    "**/research/**/.prisma": true,
    "**/*.backup": true,
    "**/research-backup-*": true
  },
  "files.watcherExclude": {
    "**/.pnpm/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/research/**/generated/**": true,
    "**/research/**/.prisma/**": true,
    "**/*.backup": true,
    "**/research-backup-*/**": true
  },
  "prisma.fileWatcher": false,
  "prisma.schemaPath": "./api/prisma/schema.prisma"
}
